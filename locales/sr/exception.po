# in Dotclear\Exception\ExceptionEnum
msgid "Bad Request"
msgstr "Лош захтев"

# in Dotclear\Exception\ExceptionEnum
msgid "Blog handling error"
msgstr "Грешка при руковању блога"

# in Dotclear\Exception\ExceptionEnum
msgid "Application configuration error"
msgstr "Грешка у конфигурацији апликације"

# in Dotclear\Exception\ExceptionEnum
msgid "Conflict"
msgstr "Конфликт"

# in Dotclear\Exception\ExceptionEnum
msgid "Application context error"
msgstr "Контекстуални грешка апликације"

