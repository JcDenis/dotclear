msgstr "Sait ei ole ajutiselt saadaval"

# in Dotclear\Exception\ExceptionEnum
msgid "Bad Request"
msgstr "Halb taotlus"

# in Dotclear\Exception\ExceptionEnum
msgid "Blog handling error"
msgstr "Blogi käitlemisviga"

# in Dotclear\Exception\ExceptionEnum
msgid "Application configuration error"
msgstr "Rakenduse konfiguratsiooniviga"

# in Dotclear\Exception\ExceptionEnum
msgid "Conflict"
msgstr "Konflikt"

# in Dotclear\Exception\ExceptionEnum
msgid "Application context error"
msgstr "Rakenduskonteksti viga"

# in Dotclear\Exception\ExceptionEnum
msgid "Database connection error"
msgstr "Andmebaasiühenduse viga"

# in Dotclear\Exception\ExceptionEnum
msgid "Not Found"
msgstr "Ei leitud"

# in Dotclear\Exception\ExceptionEnum
msgid "Precondition Failed"
msgstr "Eeltingimus ebaõnnestus"

# in Dotclear\Exception\ExceptionEnum
